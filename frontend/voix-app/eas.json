{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "production",
          "EXPO_USE_FAST_RESOLVER": "1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
